# Supabase 配置模板
# OPENAI_API
OPENAI_API_KEY=

# Supabase URL - 项目设置 > API > Project URL  
SUPABASE_URL=

# Supabase Anon Key - 项目设置 > API > anon public
SUPABASE_KEY=
# 数据库连接字符串 - 项目设置 > Database > Connection String > URI
# 格式: postgresql://postgres:[password]@db.[project-id].supabase.co:5432/postgres
SUPABASE_DB_STRING=
# 可选：开发环境配置
DEBUG=True
LOG_LEVEL=INFO